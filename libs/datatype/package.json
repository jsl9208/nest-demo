{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "mongoose": "^5.11.18"
  },
  "name": "@kesci/datatype",
  "author": "onichandame",
  "module": "dist/datatype.esm.js",
  "size-limit": [
    {
      "path": "dist/datatype.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/datatype.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@nestjs-query/query-graphql": "^0.27.0",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/mongoose": "^7.2.4"
  }
}
